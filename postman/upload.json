{
  "info": {
    "_postman_id": "7a62cc91-55db-4c56-8f63-5a1e7c1b2f42",
    "name": "Upload Multiple Images API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "_exporter_id": "insightdelivered-collection"
  },
  "item": [
    {
      "name": "Upload Multiple Images",
      "request": {
        "method": "POST",
        "header": [],
        "url": {
          "raw": "http://localhost:8080/upload/images?tenantname=Insight&username=Sankaravel",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "upload",
            "images"
          ],
          "query": [
            {
              "key": "tenantname",
              "value": "Insight"
            },
            {
              "key": "username",
              "value": "Sankaravel"
            }
          ]
        },
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "images",
              "type": "file",
              "src": [
                "C:/path/to/image1.png",
                "C:/path/to/image2.jpg"
              ]
            }
          ]
        },
        "description": "Uploads multiple image files to a network path. The folders are dynamically created using tenantname and username if they do not exist."
      },
      "response": []
    }
  ]
}
